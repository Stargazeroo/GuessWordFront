<form id="regForm" method="POST">
<h1>Registration</h1>
    <% for (var item in this) { %>
        <p><%=this[item].explText%><input id="<%=this[item].id%>" type="<%=this[item].type%>" name="<%=this[item].name%>">
        <% if ((this[item].id == 'login') || (this[item].id == 'mail_id') || (this[item].id == 'repass_id')) { %><i class="icon-remove"></i><i class="icon-ok"></i><% } %>
        </p>
    <% } %>
</form>
